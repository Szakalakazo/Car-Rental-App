<aura:component description="TRC_CustomImage" implements="forceCommunity:availableForAllPageTypes,flexipage:availableForAllPageTypes, force:hasRecordId"
                access="global">


    <aura:attribute name="currentImageURL" type="String" access="global"/>



    <lightning:carouselImage src="{!v.currentImageURL}"
                             data-index="{!v.currentImageURL}"
                             alternativeText="This is a card"
                             onclick="{!c.navigate}">

    </lightning:carouselImage>


</aura:component>
